#include "traffic_automatic.h"

void fsm_automatic_run(){
	switch(state_1){
	case INIT:
		state_1 = AUTO_RED;
		//settimer
		break;
	case AUTO_RED:
		HAL_GPIO_WritePin(GPIOA, , SET);
		break;
	case AUTO_AMBER:
		break;
	case AUTO_GREEN:
		break;
	}
}
