#include "buzzer.h"

TIM_HandleTypeDef htim3;

void buzzerRun(){
	switch(pedes_state){
	case PEDES_GREEN:
		if (timer6_flag ==1){
			setTimer6()
		}
		break;
	default:
		break;
	}
	__HAL_TIM_SetCompare (&htim3,TIM_CHANNEL_1,value);
	setTimer6(10+value*10);
}
